{
  "version": 2,
  "builds": [
    {
      "src": "pages/api/search.js",
      "use": "@vercel/node"
    },
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/search",
      "dest": "/api/search.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
